<?xml version="1.0" encoding="utf-8"?>

<RelativeLayout
    android:id="@+id/rl_content_container"
    xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/common_no_content">

    <RelativeLayout
        android:id="@+id/rl_container"
        android:layout_width="match_parent"
        android:layout_height="match_parent">

        <ScrollView
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:orientation="vertical">

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:orientation="vertical">

                <RelativeLayout
                    android:id="@+id/rl_add_profile_photo_container"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content">

                    <View
                        android:layout_width="match_parent"
                        android:layout_height="1dp"
                        android:layout_alignParentBottom="true"
                        android:background="@color/common_separator_gray"/>
                </RelativeLayout>

                <RelativeLayout
                    android:id="@+id/rl_add_profile_name_container"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content">

                    <View
                        android:layout_width="match_parent"
                        android:layout_height="1dp"
                        android:layout_alignParentBottom="true"
                        android:background="@color/common_separator_gray"/>
                </RelativeLayout>

                <RelativeLayout
                    android:id="@+id/rl_add_profile_department_container"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content">

                    <View
                        android:layout_width="match_parent"
                        android:layout_height="1dp"
                        android:layout_alignParentBottom="true"
                        android:background="@color/common_separator_gray"/>
                </RelativeLayout>

                <RelativeLayout
                    android:id="@+id/rl_add_profile_level_container"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content">

                    <View
                        android:layout_width="match_parent"
                        android:layout_height="1dp"
                        android:layout_alignParentBottom="true"
                        android:background="@color/common_separator_gray"/>
                </RelativeLayout>


                <RelativeLayout
                    android:id="@+id/rl_add_profile_number_container"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:background="@color/common_white">

                    <View
                        android:layout_width="match_parent"
                        android:layout_height="1dp"
                        android:layout_alignParentBottom="true"
                        android:background="@color/common_separator_gray"/>
                </RelativeLayout>

                <RelativeLayout
                    android:id="@+id/rl_add_profile_manager_container"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:background="@color/common_white">

                    <View
                        android:layout_width="match_parent"
                        android:layout_height="1dp"
                        android:layout_alignParentBottom="true"
                        android:background="@color/common_separator_gray"/>
                </RelativeLayout>


                <View
                    android:id="@+id/vi_separator_phone_top"
                    android:layout_width="match_parent"
                    android:layout_height="1dp"
                    android:layout_marginTop="10dp"
                    android:background="@color/common_separator_gray"/>

                <RelativeLayout
                    android:id="@+id/rl_add_profile_add_phone_container"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:orientation="vertical"/>
                <LinearLayout
                    android:id="@+id/ll_add_profile_phone_container"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:orientation="vertical"/>



                <View
                    android:id="@+id/vi_separator_phone_bottom"
                    android:layout_width="match_parent"
                    android:layout_height="1dp"
                    android:background="@color/common_separator_gray"/>

                <View
                    android:id="@+id/vi_separator_email_top"
                    android:layout_width="match_parent"
                    android:layout_height="1dp"
                    android:layout_marginTop="10dp"
                    android:background="@color/common_separator_gray"/>
                <RelativeLayout
                    android:id="@+id/rl_add_profile_add_email_container"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"/>
                <LinearLayout
                    android:id="@+id/ll_add_profile_email_container"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:orientation="vertical"/>



                <View
                    android:id="@+id/vi_separator_email_bottom"
                    android:layout_width="match_parent"
                    android:layout_height="1dp"
                    android:background="@color/common_separator_gray"/>

                <RelativeLayout
                    android:id="@+id/rl_add_profile_username_container"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content">

                    <View
                        android:layout_width="match_parent"
                        android:layout_height="1dp"
                        android:background="@color/common_separator_gray"/>
                </RelativeLayout>

                <View

                    android:layout_width="match_parent"
                    android:layout_height="1dp"
                    android:background="@color/common_separator_gray"/>

                <RelativeLayout
                    android:id="@+id/rl_add_profile_password_container"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"/>

                <View
                    android:layout_width="match_parent"
                    android:layout_height="1dp"
                    android:background="@color/common_separator_gray"/>

                <RelativeLayout
                    android:id="@+id/rl_add_profile_authority_container"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"/>

                <View
                    android:layout_width="match_parent"
                    android:layout_height="1dp"
                    android:background="@color/common_separator_gray"/>

                <LinearLayout
                    android:id="@+id/rl_add_profile_delete_container"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:orientation="vertical"/>
            </LinearLayout>
        </ScrollView>
    </RelativeLayout>
</RelativeLayout>